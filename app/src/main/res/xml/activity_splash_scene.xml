<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:autoTransition="animateToEnd"
        motion:duration="5000"
        motion:pathMotionArc="startHorizontal">

        <KeyFrameSet>
            <KeyPosition
                motion:framePosition="5"
                motion:motionTarget="@id/iv_outline_circle"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.503"
                motion:percentY="0.28"
                />
            <KeyPosition
                motion:framePosition="25"
                motion:motionTarget="@id/iv_outline_circle"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.915"
                motion:percentY="0.5"
                motion:pathMotionArc="startVertical"
                />
            <KeyPosition
                motion:framePosition="45"
                motion:motionTarget="@id/iv_outline_circle"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.5"
                motion:percentY="0.72"
                motion:pathMotionArc="startHorizontal"
                />
            <KeyPosition
                motion:framePosition="65"
                motion:motionTarget="@id/iv_outline_circle"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.086"
                motion:percentY="0.5"
                motion:pathMotionArc="startVertical"/>

            <KeyPosition
                motion:framePosition="15"
                motion:motionTarget="@id/iv_outline_circle2"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.503"
                motion:percentY="0.28"
                />
            <KeyPosition
                motion:framePosition="35"
                motion:motionTarget="@id/iv_outline_circle2"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.915"
                motion:percentY="0.5"
                motion:pathMotionArc="startVertical"
                />
            <KeyPosition
                motion:framePosition="55"
                motion:motionTarget="@id/iv_outline_circle2"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.5"
                motion:percentY="0.72"
                motion:pathMotionArc="startHorizontal"
                />
            <KeyPosition
                motion:framePosition="75"
                motion:motionTarget="@id/iv_outline_circle2"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.086"
                motion:percentY="0.5"
                motion:pathMotionArc="startVertical"/>

            <KeyPosition
                motion:framePosition="25"
                motion:motionTarget="@id/iv_outline_circle3"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.503"
                motion:percentY="0.28"
                />
            <KeyPosition
                motion:framePosition="45"
                motion:motionTarget="@id/iv_outline_circle3"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.915"
                motion:percentY="0.5"
                motion:pathMotionArc="startVertical"
                />
            <KeyPosition
                motion:framePosition="65"
                motion:motionTarget="@id/iv_outline_circle3"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.5"
                motion:percentY="0.72"
                motion:pathMotionArc="startHorizontal"
                />
            <KeyPosition
                motion:framePosition="85"
                motion:motionTarget="@id/iv_outline_circle3"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.086"
                motion:percentY="0.5"
                motion:pathMotionArc="startVertical"/>

            <KeyPosition
                motion:framePosition="35"
                motion:motionTarget="@id/iv_outline_circle4"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.503"
                motion:percentY="0.28"
                />
            <KeyPosition
                motion:framePosition="55"
                motion:motionTarget="@id/iv_outline_circle4"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.915"
                motion:percentY="0.5"
                motion:pathMotionArc="startVertical"
                />
            <KeyPosition
                motion:framePosition="75"
                motion:motionTarget="@id/iv_outline_circle4"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.5"
                motion:percentY="0.72"
                motion:pathMotionArc="startHorizontal"
                />
            <KeyPosition
                motion:framePosition="90"
                motion:motionTarget="@id/iv_outline_circle4"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.086"
                motion:percentY="0.5"
                motion:pathMotionArc="startVertical"/>

            <KeyPosition
                motion:framePosition="45"
                motion:motionTarget="@id/iv_outline_circle5"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.503"
                motion:percentY="0.28"
                />
            <KeyPosition
                motion:framePosition="65"
                motion:motionTarget="@id/iv_outline_circle5"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.915"
                motion:percentY="0.5"
                motion:pathMotionArc="startVertical"
                />
            <KeyPosition
                motion:framePosition="85"
                motion:motionTarget="@id/iv_outline_circle5"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.5"
                motion:percentY="0.72"
                motion:pathMotionArc="startHorizontal"
                />
            <KeyPosition
                motion:framePosition="95"
                motion:motionTarget="@id/iv_outline_circle5"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.086"
                motion:percentY="0.5"
                motion:pathMotionArc="startVertical"/>

            <KeyAttribute
                motion:framePosition="0"
                motion:motionTarget="@id/iv_outline_circle"
                android:alpha="0.5"
                />
            <KeyAttribute
                motion:framePosition="0"
                motion:motionTarget="@id/iv_outline_circle2"
                android:alpha="0.5"
                />
            <KeyAttribute
                motion:framePosition="0"
                motion:motionTarget="@id/iv_outline_circle3"
                android:alpha="0.5"
                />
            <KeyAttribute
                motion:framePosition="0"
                motion:motionTarget="@id/iv_outline_circle4"
                android:alpha="0.5"
                />
            <KeyAttribute
                motion:framePosition="0"
                motion:motionTarget="@id/iv_outline_circle5"
                android:alpha="0.5"
                />
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/iv_outline_circle"
            motion:layout_constraintEnd_toEndOf="@+id/imageView"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView"
            motion:layout_constraintTop_toTopOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="@+id/imageView" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            android:id="@+id/iv_outline_circle"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintEnd_toEndOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="@+id/imageView"
            motion:layout_constraintTop_toTopOf="@+id/imageView"
            motion:layout_constraintBottom_toTopOf="@+id/imageView" />
        <Constraint
            android:id="@+id/iv_outline_circle2"
            android:layout_height="50dp"
            android:layout_width="50dp"
            motion:layout_constraintEnd_toEndOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="@+id/imageView"
            motion:layout_constraintTop_toTopOf="@+id/imageView"
            motion:layout_constraintBottom_toTopOf="@+id/imageView" />
        <Constraint
            android:id="@+id/iv_outline_circle3"
            android:layout_height="50dp"
            android:layout_width="50dp"
            motion:layout_constraintEnd_toEndOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="@+id/imageView"
            motion:layout_constraintTop_toTopOf="@+id/imageView"
            motion:layout_constraintBottom_toTopOf="@+id/imageView" />
        <Constraint
            android:id="@+id/iv_outline_circle4"
            android:layout_height="50dp"
            android:layout_width="50dp"
            motion:layout_constraintEnd_toEndOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="@+id/imageView"
            motion:layout_constraintTop_toTopOf="@+id/imageView"
            motion:layout_constraintBottom_toTopOf="@+id/imageView" />
        <Constraint
            android:id="@+id/iv_outline_circle5"
            android:layout_height="50dp"
            android:layout_width="50dp"
            motion:layout_constraintEnd_toEndOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="@+id/imageView"
            motion:layout_constraintTop_toTopOf="@+id/imageView"
            motion:layout_constraintBottom_toTopOf="@+id/imageView" />
    </ConstraintSet>
</MotionScene>